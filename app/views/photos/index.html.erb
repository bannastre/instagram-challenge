<h1>Photos:</h1>

<ul id="photos">
  <% @photos.each do |photo| %>

    <li> <p><%= photo.name %></p>

      <div class="" id="photo_id_<%= photo.id %>">
        <img src="<%= photo.photo_file %>" alt="<%= photo.name %>">
      </div>

      <div class="" id="photo_comments_<%= photo.id %>">
        <%= button_to "Comment", photo_path(photo.id), method: :get %>
      </div>

    </li>
  <% end %>
</ul>
